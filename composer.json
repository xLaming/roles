{
    "name": "xlaming/roles",
    "description": "Powerful package for handling roles and permissions in Laravel 6.",
    "abandoned": true,
    "keywords": [
        "roles",
        "permissions",
        "acl",
        "auth",
        "laravel",
        "illuminate"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ultraware"
        },
        {
            "name": "Paulo Rodrigues"
        },
        {
            "name": "Roman Bičan"
        }
    ],
    "require": {
        "php": ">=7.1.3",
    },
    "autoload": {
        "psr-4": {
            "Ultraware\\Roles\\": "src/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "4.*",
        "orchestra/database": "4.*",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.5",
        "satooshi/php-coveralls": "2.*",
        "sebastian/environment": "^5.1"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/User.php"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Ultraware\\Roles\\RolesServiceProvider"
            ]
        }
    }
}
